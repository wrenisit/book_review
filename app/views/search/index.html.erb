<%= @book["title"] %>
<% @book["author_name"].each do |author| %>
  <%= author %>
<% end %>
<section class="reviews">
  <% @reviews.each do |review| %>
    <section class="review"
      <h3>Review: <%= review["summary"] %>
      <h4>Review Publication Date: <%= review["publication_dt"] %></h4>
    </section>
  <% end %>
</section>
